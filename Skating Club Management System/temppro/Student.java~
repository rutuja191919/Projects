
import java.awt.*;
import java.awt.event.*;
import javax.swing.*;

class Student extends JFrame implements ActionListener
{
  JButton  Stud,edit,dltStud,view, logout;
  public Student ()
  {
    super ("Admin Main Page");
       	
  JLabel background=new JLabel(new ImageIcon(ClassLoader.getSystemResource ("pics/sc1.JPG")));
				  this.add(background);
    this.setSize (1320, 730);


    this.setDefaultCloseOperation (EXIT_ON_CLOSE);


    Stud = new JButton ("NEW Registration of Student");
    edit= new JButton ("Edit Student Record");
    dltStud= new JButton ("Delete Student");
    view = new JButton ("View Student Record");
    logout = new JButton ("<<back");

   /* Admin.setBounds(280,130,180,39);
    edit.setBounds(280,200,180,39);
    fee.setBounds(280,270,180,39);

    Stud.setBounds(540,130,180,39);   
    dltAdmin.setBounds(540,200,180,39);    
    view.setBounds(540,270,180,39);

    dltStud.setBounds(800,200,180,39);
    attend.setBounds(800,130,180,39);
    serv.setBounds(800,270,180,39);*/

    Stud.setBounds(540,130,180,39);   
    edit.setBounds(540,200,180,39);    
    dltStud.setBounds(540,270,180,39);
    view.setBounds(540,340,180,39);
 
    logout.setBounds(840,520,210,39);


    background.add (Stud);
    background.add (edit);
    background.add (view);
    background.add (dltStud);
    background.add (logout);
   
    Stud.addActionListener (this);
    edit.addActionListener (this);
    dltStud.addActionListener (this);
    view.addActionListener (this); 
    logout.addActionListener (this);

this.setVisible (true);

  }
  public static void main (String s[])
  {
    new Student ();

  }
  public void actionPerformed (ActionEvent e3)
  {
	if (e3.getSource () == logout)
      {
	new ManMain1 ();
	dispose();
	}
	else if(e3.getSource() == Stud)
	{
        new AddStud();
	dispose();
	}
	else if(e3.getSource()==edit)
	{
	new EditRec();
	dispose();
	}
	else if(e3.getSource()==view)
	{
	new ViewRec();
	dispose();
	}
	else if(e3.getSource()==dltStud)
	{
	new DStud();
	dispose();
	}
  }
}
